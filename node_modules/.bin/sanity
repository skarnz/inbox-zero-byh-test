#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/sanity@3.90.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.29_@types+react@19.0.10_im_58bb82498895185c7de46ca175be4534/node_modules/sanity/bin/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/sanity@3.90.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.29_@types+react@19.0.10_im_58bb82498895185c7de46ca175be4534/node_modules/sanity/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/sanity@3.90.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.29_@types+react@19.0.10_im_58bb82498895185c7de46ca175be4534/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/sanity@3.90.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.29_@types+react@19.0.10_im_58bb82498895185c7de46ca175be4534/node_modules/sanity/bin/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/sanity@3.90.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.29_@types+react@19.0.10_im_58bb82498895185c7de46ca175be4534/node_modules/sanity/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/sanity@3.90.0_@emotion+is-prop-valid@1.2.2_@types+node@22.15.29_@types+react@19.0.10_im_58bb82498895185c7de46ca175be4534/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../sanity/bin/sanity" "$@"
else
  exec node  "$basedir/../sanity/bin/sanity" "$@"
fi
