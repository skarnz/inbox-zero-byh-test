#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/braintrust@0.0.205_@aws-sdk+credential-provider-web-identity@3.750.0_openai@5.0.1_ws@8._d25ea35b3bc516f1497ae799c97d3741/node_modules/braintrust/dist/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/braintrust@0.0.205_@aws-sdk+credential-provider-web-identity@3.750.0_openai@5.0.1_ws@8._d25ea35b3bc516f1497ae799c97d3741/node_modules/braintrust/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/braintrust@0.0.205_@aws-sdk+credential-provider-web-identity@3.750.0_openai@5.0.1_ws@8._d25ea35b3bc516f1497ae799c97d3741/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/braintrust@0.0.205_@aws-sdk+credential-provider-web-identity@3.750.0_openai@5.0.1_ws@8._d25ea35b3bc516f1497ae799c97d3741/node_modules/braintrust/dist/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/braintrust@0.0.205_@aws-sdk+credential-provider-web-identity@3.750.0_openai@5.0.1_ws@8._d25ea35b3bc516f1497ae799c97d3741/node_modules/braintrust/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/braintrust@0.0.205_@aws-sdk+credential-provider-web-identity@3.750.0_openai@5.0.1_ws@8._d25ea35b3bc516f1497ae799c97d3741/node_modules:/Users/skarnz/inbox_manager_demo/inbox-zero/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../braintrust/dist/cli.js" "$@"
else
  exec node  "$basedir/../braintrust/dist/cli.js" "$@"
fi
